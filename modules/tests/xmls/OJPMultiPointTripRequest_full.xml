        <!-- TripPolicyGroup -->
        <NumberOfResults>10<!--
          @role Tells the server to return at most the mentioned number of results in its response.
          @type integer
          @min 1
          @max 100
          @required
        --></NumberOfResults>
        <IgnoreRealtimeData>false<!--
          @role
          @optional
          @type boolean
        --></IgnoreRealtimeData>
        <ImmediateTripStart>false<!--
          @role
          @optional
          @type boolean
        --></ImmediateTripStart>
